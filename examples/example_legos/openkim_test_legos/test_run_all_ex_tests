       "   |-- ex_model_Ar_P_MLJ_NEIGH_RVEC_F"   \
       "   |-- ex_model_Ar_P_Morse"              \
       "   |"
printf "\n"
printf "NOTE: Only four examples are executed here because all other permutations produce identical results\n"
printf "\n"
divider "ex_test_Ar_multiple_models --- ex_model_ArNe_P_MLJ_NEIGH_RVEC_F -and- ex_model_Ar_P_MLJ_C"
(cd ex_test_Ar_multiple_models && printf "ex_model_ArNe_P_MLJ_NEIGH_RVEC_F ex_model_Ar_P_MLJ_C" |  ./ex_test_Ar_multiple_models) >&$redirect && MODEL_NUM=$(($MODEL_NUM + 1))

divider "ex_test_Ar_multiple_models --- ex_model_Ar_P_MLJ_C -and- ex_model_Ar_P_LJ"
(cd ex_test_Ar_multiple_models && printf "ex_model_Ar_P_MLJ_C ex_model_Ar_P_LJ"                 |  ./ex_test_Ar_multiple_models) >&$redirect && MODEL_NUM=$(($MODEL_NUM + 1))

divider "ex_test_Ar_multiple_models --- ex_model_Ar_P_MLJ_C -and- ex_model_Ar_P_Morse"
(cd ex_test_Ar_multiple_models && printf "ex_model_Ar_P_MLJ_C ex_model_Ar_P_Morse"              |  ./ex_test_Ar_multiple_models) >&$redirect && MODEL_NUM=$(($MODEL_NUM + 1))

divider "ex_test_Ar_multiple_models --- ex_model_Ar_P_LJ -and- ex_model_Ar_P_Morse"
(cd ex_test_Ar_multiple_models && printf "ex_model_Ar_P_LJ ex_model_Ar_P_Morse"                 |  ./ex_test_Ar_multiple_models) >&$redirect && MODEL_NUM=$(($MODEL_NUM + 1))
footer

